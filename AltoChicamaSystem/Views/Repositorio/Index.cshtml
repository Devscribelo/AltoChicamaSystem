@{
    ViewData["Title"] = "Repositorio";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}
<link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet" />
<link rel="stylesheet" href="~/css/empresa.css">

<!-- JS Libraries -->
<script src="~/lib/jquery-3.6.0/jquery-3.6.0.min.js"></script>

<!-- JS Personalizado -->
<script src="~/config.js"></script>

<section class="home">
    <main>
        <div class="row" style="margin:0px">
            <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
                <h2 class="h2" style="font-size:1.5rem">Documentos</h2>
            </div>
            <div class="container">
                
            </div>

            <div style="padding-bottom: 9px;">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="table-responsive">
                            <table class="table table-sm custom-table" id="table_empresa">
                                <thead style="color: #023E73;">
                                    <tr>
                                        <th>ID</th>
                                        <th>Nombre del Documento</th>
                                        <th>Nombre de Empresa</th>
                                        <th>Ver PDF</th>
                                        <th>Acciones</th>
                                    </tr>
                                </thead>
                                <tbody id="tb_empresa"></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </main>

    <script defer src="https://use.fontawesome.com/releases/v5.15.4/js/all.js" integrity="sha384-rOA1PnstxnOBLzCLMcre8ybwbTmemjzdNlILg8O7z1lUkLXozs4DHonlDtnE7fpc" crossorigin="anonymous"></script>
    <script type="text/javascript" src="~/lib/jquery-3.6.0/jquery-3.6.0.min.js"></script>
    <script src="~/js/js_jquery.dataTables.min.js"></script>
    <script src="~/config.js"></script>
    <script src="~/js/repositorio.js"></script>
</section>